{
  "name": "vite-boilerplate-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "start": "concurrently  npm:dummy-server npm:dev",
    "dummy-server": "node src/api/mocks/jsonServer/index.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview --port 8000",
    "test": "vitest",
    "test:ci": "vitest run",
    "test:ui": "vitest --ui",
    "format": "prettier --write \"src/**/*.ts\" ",
    "lint": "eslint ./src --fix",
    "pre-commit-lint": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "validate-branch-name": {
    "pattern": "^(master|dev|uat){1}$|^(feature|bugfix|hotfix|release)/[A-Z]{1,}-[0-9]{1,}",
    "errorMsg": "Please follow the branch naming convention, eg: feature/SG-1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,less}": [
      "prettier --ignore-path ./.prettierignore --write "
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@react-oauth/google": "^0.9.0",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tanstack/react-query": "^4.29.3",
    "@tanstack/react-query-devtools": "^4.29.3",
    "@visx/axis": "^3.2.0",
    "@vitest/ui": "0.30.0",
    "axios": "^1.3.4",
    "date-fns": "^2.30.0",
    "dompurify": "^3.0.5",
    "formik": "^2.4.1",
    "history": "^5.3.0",
    "react": "^18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "^18.2.0",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@heroicons/react": "^2.0.17",
    "@material-tailwind/react": "^2.0.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.2",
    "@tanstack/eslint-plugin-query": "^4.29.4",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@visx/event": "^3.0.1",
    "@visx/grid": "^3.2.0",
    "@visx/group": "^3.0.0",
    "@visx/legend": "^3.2.0",
    "@visx/mock-data": "^3.0.0",
    "@visx/pattern": "^3.0.0",
    "@visx/react-spring": "^3.2.0",
    "@visx/responsive": "^3.0.0",
    "@visx/scale": "^3.2.0",
    "@visx/shape": "^3.2.0",
    "@visx/tooltip": "^3.1.2",
    "@visx/xychart": "^3.2.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "@vitest/coverage-istanbul": "^0.30.1",
    "autoprefixer": "^10.4.14",
    "classnames": "^2.3.2",
    "clean-css": "^5.3.2",
    "concurrently": "^8.0.1",
    "d3-time-format": "^4.1.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-testing-library": "^5.10.3",
    "eslint-plugin-unused-imports": "^2.0.0",
    "framer-motion": "^10.15.0",
    "happy-dom": "^9.18.3",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "json-server": "^0.17.3",
    "jwt-decode": "^3.1.2",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.1",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.7",
    "react-ga4": "^2.1.0",
    "react-test-renderer": "^18.2.0",
    "react-toastify": "^9.1.2",
    "tailwindcss": "^3.3.1",
    "typescript": "^4.9.3",
    "validate-branch-name": "^1.3.0",
    "vite": "^4.3.9",
    "vite-plugin-checker": "^0.5.6",
    "vite-tsconfig-paths": "^4.0.8",
    "vitest": "^0.30.1"
  }
}
